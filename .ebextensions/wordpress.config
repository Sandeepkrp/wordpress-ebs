    LOGGED_IN_KEY: 'default'
    NONCE_KEY: 'default'
    AUTH_SALT: 'default'
    SECURE_AUTH_SALT: 'default'
    NONCE_SALT: 'default'
    RDS_DB_NAME: 'rdsdb'
    RDS_HOSTNAME: 'default'
    RDS_PORT: '3306'
    RDS_USERNAME: 'rdsuser'
    RDS_PASSWORD: 'rdspassword'
